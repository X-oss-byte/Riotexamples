{
  "name": "riot-ie-example",
  "version": "1.0.0",
  "description": "Riot.js 4 example App working on IE11",
  "private": true,
  "scripts": {
    "start": "npm run watch & npm run server",
    "build": "rollup -c",
    "watch": "rollup -c -w 'src/**'",
    "server": "browser-sync start --files 'dist/*, index.html' --server"
  },
  "dependencies": {
    "marked": "^0.7.0",
    "riot": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@riotjs/compiler": "^5.0.0",
    "browser-sync": "^2.26.7",
    "rollup": "^1.20.3",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-riot": "^4.0.0"
  },
  "license": "MIT"
}
